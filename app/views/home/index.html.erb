<h1>Welcome to Run with Me</h1>
<% if !user_signed_in? %>
  <%=link_to "Join", new_user_registration_path%>
  <%=link_to "Sing-in", new_user_session_path %>
<%else%>
  <%= link_to "Sign out", destroy_user_session_path, :method => :delete %>
  <%= link_to 'Profile', user_path(@user), class: "button" %>
<% end %>